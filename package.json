{
  "name": "my-app",
  "productName": "my-app",
  "version": "0.0.1",
  "description": "test electron app",
  "private": true,
  "scripts": {
    "start:socket-server": "nodemon server.js",
    "start:web": "webpack-dev-server --config webpack.dev.js",
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "echo \"No linting configured\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dragmove/_20190619_ELECTRON.git"
  },
  "keywords": [],
  "author": "dragmove",
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "my_app"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  },
  "nodemonConfig": {
    "ignore": [
      "serviceworker.js",
      "src/*"
    ]
  },
  "dependencies": {
    "electron-squirrel-startup": "^1.0.0",
    "nodemon": "^1.19.1",
    "ws": "^7.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.4.5",
    "@electron-forge/cli": "^6.0.0-beta.39",
    "@electron-forge/maker-deb": "^6.0.0-beta.39",
    "@electron-forge/maker-rpm": "^6.0.0-beta.39",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.39",
    "@electron-forge/maker-zip": "^6.0.0-beta.39",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.2",
    "electron": "5.0.4",
    "eslint": "^5.16.0",
    "eslint-loader": "^2.1.2",
    "lodash": "^4.17.11",
    "terser-webpack-plugin": "^1.3.0",
    "webpack": "^4.28.4",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.7.2"
  }
}
